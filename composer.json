    "type": "library",
    "description": "This is the laravel api auth system intended for the longman/telegram-bot",
    "license": "MIT",
    "authors": [
        {
            "name": "CaliforniaMountainSnake",
            "email": "CaliforniaMountainSnake1@yandex.ru"
        }
    ],
    "require": {
        "php": "^7.2",
